function printPixieStyle(style)
browseFields(style,'style');
end


function browseFields(struc,baseName)
tabLength=8;% number of characters per tab
totalTabDist=6;% final distance of structures values in tabs
fields=fieldnames(struc);
for ff = 1:length(fields)
    f=struc.(fields{ff});
    bn=[baseName '.' fields{ff}];
    bnTabNum=floor((length(bn)+3)/tabLength)+1;%distance of first tab after structure name. 
    totalTabNum=totalTabDist-bnTabNum;
    tab=repmat('\t',1,totalTabNum);
    if isstruct(f)
        browseFields(f,bn);
    else
        if isnumeric(f)
            if isvector(f)
                fprintf([bn ' = ' tab '[' num2str(f) '];\n']);
            else
                if length(size(f))==2
                    fprintf([bn ' = ' tab '[' num2str(f(1,:)) ';...\n']);
                    for rr = 2:size(f,1)-1
                        fprintf([repmat('\t',1,totalTabDist) num2str(f(rr,:)) ';...\n']);
                    end
                    fprintf([repmat('\t',1,totalTabDist) num2str(f(end,:)) '];\n']);
                else
                    error([bn ' is a matrix of more than 2 dimensions. We cannot handle this type of data.']);
                end
            end
        elseif ischar(f)
            fprintf([bn ' = ' tab '' f ''';\n']);
        end
    end
end
end